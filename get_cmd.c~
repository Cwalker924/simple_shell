int get_command(char c, /*va_list *ap*/)
{
	int i;
	find_cmd flags[] = {
		{"ls", /*env*/},
		{'s', p_string},
		{'b', p_binary},
		{'r', p_rev},
		{'R', ____},
		{NULL, NULL}
	};

	for (i = 0; flags[i].letter != NULL; i++)
	{
		if (flags[i].letter == c)
		{
			return (flags[i].prnt(ap));
		}
	}
	if (c == '%')
	{
		_putchar('%');
		return (1);
	}
	return (0);

}
